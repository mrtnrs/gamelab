    "next": "^15.2.0",
    "next-themes": "^0.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^4.12.0",
    "vercel": "^41.3.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.0.1"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^0.5.8",
    "@types/node": "^20",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^8",
    "eslint-config-next": "^15.2.0",
    "postcss": "^8",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.1"
